{
  "name": "ws-nats",
  "version": "2.0.1",
  "description": "A browser and NodeJS websocket client library for NATS",
  "main": "src/nats.js",
  "scripts": {
    "rollup_build": "rimraf dist && rollup -c",
    "rollup_dev": "rimraf dist && rollup -c -w",
    "webpack_build": "rimraf dist && webpack --config webpack.config.js --mode=production --display-modules",
    "webpack_dev": "rimraf dist && webpack --config webpack.config.js --mode=development --display-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antonioaguilar/ws-nats.git"
  },
  "keywords": [
    "websocket",
    "ws",
    "nats",
    "browser",
    "nodejs",
    "NATS",
    "messaging",
    "pubsub"
  ],
  "author": "Antonio Aguilar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antonioaguilar/ws-nats/issues"
  },
  "homepage": "https://github.com/antonioaguilar/ws-nats#readme",
  "devDependencies": {
    "rimraf": "latest",
    "rollup": "^0.63.5",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-uglify": "3.0.0",
    "uglifyjs-webpack-plugin": "latest",
    "webpack": "latest",
    "webpack-cli": "latest"
  },
  "browser": {
    "net": "./src/net.js",
    "tls": "./src/tls.js",
    "nuid": "./src/nuid.js"
  },
  "dependencies": {
  }
}
