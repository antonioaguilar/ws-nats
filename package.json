{
  "name": "ws-nats",
  "version": "2.2.1",
  "description": "A browser websocket client library for NATS",
  "main": "src/nats.js",
  "scripts": {
    "build": "rimraf dist && webpack --config webpack.config.js --mode=production --display-modules",
    "dev": "rimraf dist && webpack --config webpack.config.js --mode=development --display-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antonioaguilar/ws-nats.git"
  },
  "keywords": [
    "websocket",
    "ws",
    "nats",
    "browser",
    "nodejs",
    "NATS",
    "messaging",
    "pubsub"
  ],
  "author": "Antonio Aguilar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antonioaguilar/ws-nats/issues"
  },
  "homepage": "https://github.com/antonioaguilar/ws-nats#readme",
  "devDependencies": {
    "rimraf": "latest",
    "webpack": "latest",
    "webpack-cli": "latest"
  },
  "browser": {
    "net": "./src/net.js",
    "tls": "./src/tls.js",
    "nuid": "./src/nuid.js",
    "fs": "./src/fs.js",
    "ts-nkeys": "./src/nkeys.js"
  },
  "dependencies": {
    "sockjs-client": "latest"
  }
}
